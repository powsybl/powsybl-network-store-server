<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet author="hedhiliabd" id="2986331049277-1">
        <createTable tableName="ground">
            <column name="networkuuid" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="ground_pkey"/>
            </column>
            <column name="variantnum" type="INTEGER">
                <constraints nullable="false" primaryKey="true" primaryKeyName="ground_pkey"/>
            </column>
            <column name="id" type="VARCHAR(255)">
                <constraints nullable="false" primaryKey="true" primaryKeyName="ground_pkey"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
            <column name="fictitious" type="BOOLEAN"/>
            <column name="properties" type="TEXT"/>
            <column name="aliaseswithouttype" type="TEXT"/>
            <column name="aliasbytype" type="TEXT"/>
            <column name="position" type="TEXT"/>
            <column name="voltagelevelid" type="VARCHAR(255)"/>
            <column name="p" type="FLOAT8"/>
            <column name="q" type="FLOAT8"/>
            <column name="node" type="INTEGER"/>
            <column name="bus" type="VARCHAR(255)"/>
            <column name="connectablebus" type="VARCHAR(255)"/>
        </createTable>
        <createIndex indexName="ground_networkuuid_variantnum_voltagelevelid_idx" tableName="ground">
            <column name="networkuuid"/>
            <column name="variantnum"/>
            <column name="voltagelevelid"/>
        </createIndex>
    </changeSet>
</databaseChangeLog>
